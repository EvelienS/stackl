{
    "name": "nginx",
    "type": "application",
    "invocation": {
        "image": "ansible:operator",
        "description": "installs nginx"
    },
    "input": {
        "hostname": {
            "type": "string",
            "required": true,
            "default": ""
        }
    },
    "output": {
        "homepage-address": {
            "type": "string"
        }
    },
    "params": {
        "port": 443,
        "logdir": "/var/log/nginx.log"
    }
}